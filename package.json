{
  "name": "keylog.io",
  "version": "0.1.5",
  "description": "A express/socketio keylogger server with administrator interface",
  "license": "MIT",
  "main": "main.js",
  "bin": {
    "keylog-io": "lib/cli.js"
  },
  "files": [
    "main.js",
    "lib",
    "dist"
  ],
  "scripts": {
    "start": "node scripts/server.js",
    "dev": "concurrently \"ng serve -dop false --open\" \"npm run server\"",
    "server": "nodemon --watch ./lib ./scripts/dev-server.js",
    "prebuild": "npm run lint",
    "build": "ng build --prod",
    "demo": "node ./example/index.js",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "async": "^2.6.0",
    "commander": "^2.13.0",
    "compression": "^1.7.1",
    "devtools-detect": "^2.2.0",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.3",
    "moment": "^2.20.1",
    "mysql": "^2.15.0",
    "opn": "^5.2.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.2",
    "@angular/cdk": "^5.1.1",
    "@angular/cli": "1.6.6",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/material": "^5.1.1",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "angular2-moment": "^1.8.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "core-js": "^2.4.1",
    "dotenv": "^5.0.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.7",
    "protractor": "^5.4.1",
    "rxjs": "^5.5.6",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  }
}
